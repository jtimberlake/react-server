{
  "private": true,
  "scripts": {
    "audit": "npm audit && lerna exec --stream npm audit",
    "bootstrap": "npm i",
    "bootstrap-no-i": "lerna bootstrap && lerna link --force-local",
    "build": "lerna run build",
    "changelog": "lerna-changelog",
    "clean": "rimraf lerna-debug.log && lerna run clean",
    "debug": "cat lerna-debug.log && for d in packages/*/npm-debug.log*; do echo $d; cat $d; done",
    "lint": "lerna run lint",
    "nuke": "npm run clean && lerna clean --yes && rm -rf node_modules",
    "postinstall": "npm run bootstrap-no-i",
    "test": "lerna run test",
    "update-dependencies": "david u && lerna exec david u"
  },
  "changelog": {
    "repo": "redfin/react-server",
