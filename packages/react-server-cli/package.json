{
  "name": "react-server-cli",
  "version": "1.0.0-alpha.2",
  "description": "A command line to run react-server",
  "main": "target/index.js",
  "scripts": {
    "ava-watch": "ava --watch",
    "build": "gulp",
    "clean": "rimraf target npm-debug.log*",
    "lint": "eslint src test",
    "prepublish": "npm run build",
    "test": "ava && gulp test",
    "watch": "gulp && gulp watch"
  },
  "author": "Sasha Aickin",
  "bin": {
    "react-server": "./bin/react-server-cli"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/register": "^7.4.0",
    "babel-loader": "^8.0.5",
    "body-parser": "^1.20.3",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "cookie-parser": "1.4.3",
    "css-loader": "^2.1.1",
    "express": "^4.14.0",
    "express-state": "^1.4.0",
    "extract-css-chunks-webpack-plugin": "^4.3.0",
    "file-loader": "^3.0.1",
    "helmet": "^3.3.0",
    "json-loader": "^0.5.7",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash": "^4.16.4",
    "look-up": "^0.8.3",
    "mkdirp": "^0.5.1",
    "node-libs-browser": "^2.0.0",
    "node-sass": "^4.11.0",
    "null-loader": "~0.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pem": "^1.8.1",
    "q": "1.4.1",
    "raw-loader": "^2.0.0",
    "react-hot-loader": "^4.8.2",
    "react-server-core-middleware": "^1.0.0-alpha.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "webpack": "^4.29.6",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-stats-plugin": "^0.2.1",
    "yargs": "^3.32.0"
  },
  "devDependencies": {
    "babel-preset-react-server": "^1.0.0-alpha.2",
    "react-server": "^1.0.0-alpha.2",
    "react-server-gulp-module-tagger": "^1.0.0-alpha.2"
  },
  "ava": {
    "tap": true,
    "babel": {
      "extensions": [
        "js",
        "jsx"
      ]
    },
    "require": [
      "@babel/register"
    ],
    "files": [
      "test/**/*.js",
      "!test/fixtures/*.js"
    ]
  }
}
