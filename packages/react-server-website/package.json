    "react-dom": "~0.14.9 || ^15.3.0 || ^16.0.0",
    "react-server": "file:../react-server",
    "react-server-cli": "file:../react-server-cli",
    "react-server-data-bundle-cache": "file:../react-server-data-bundle-cache",
    "remarkable": "^1.6.2"
  }
